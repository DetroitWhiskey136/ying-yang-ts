{
  "compilerOptions": {
    "esModuleInterop": true,
    "noUnusedLocals": false,
    "target": "es2019",
    "module": "commonjs",
    "noImplicitAny": true,
    "outDir": "dist/src",
    "baseUrl": ".",
    "paths": {
    },
    "resolveJsonModule": true,
    "rootDirs": ["src", "dist/src"],
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": true,

  },
  "exclude": [
    "node_modules"
  ],

  "typedocOptions": {
    "entryPoints": ["src/index.ts"],
    "out": "docs",
    "name": "Yin Yang TS Documentation",
    "json": "docs/docs.json",
    "excludePrivate": true,
    "excludeInternal": true,
    "disableOutputCheck": true,
    "includeVersion": true,
    "disableSources": true,
    "readme": "none",
    "defaultCategory": "Misc",
    "hideGenerator": true,
    "toc": [],
    "listInvalidSymbolLinks": true,
    "emit": true,
    "theme": "./node_modules/typedoc-darker-theme/bin/default",
    "highlightTheme": "material-theme-darker",
  }
}
