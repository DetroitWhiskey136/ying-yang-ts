{
  "name": "yin-yang-ts",
  "version": "1.0.0",
  "description": "This is a wip concept for typescript stuff in regards to yin-yang-class",
  "main": "dist/src/bot.js",
  "scripts": {
    "lint": "eslint --ext ts src",
    "lint:fix": "eslint --fix --ext ts src",
    "build": "tsc",
    "build:fast": "esbuild --outdir=dist --platform=node --target=node14 --format=cjs `find src -type f -name '*.ts'`",
    "prebuild": "rimraf dist",
    "start-dev": "yarn build:fast && cross-env NODE_ENV=development node .",
    "watch": "yarn prebuild && cross-env NODE_ENV=development tsc-watch --onSuccess \"node .\"",
    "watch:fast": "esbuild --outdir=dist --platform=node --target=node14 --format=cjs --watch `find src -type f -name '*.ts'`",
    "run:dev": "cross-env NODE_ENV=development tsnd --ignore-watch node_modules --respawn --transpile-only --watch src,.env --no-notify src/bot.ts",
    "run:pro": "yarn build && cross-env NODE_ENV=production node .",
    "docs:build": "typedoc --tsconfig tsconfig.json",
    "docs:watch": "typedoc --watch --tsconfig tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DetroitWhiskey136/ying-yang-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "private": true,
  "bugs": {
    "url": "https://github.com/DetroitWhiskey136/ying-yang-ts/issues"
  },
  "homepage": "https://github.com/DetroitWhiskey136/ying-yang-ts#readme",
  "dependencies": {
    "@discordjs/voice": "^0.3.1",
    "@sentry/node": "^5.27.1",
    "@sentry/tracing": "^5.27.1",
    "@types/color-thief-node": "^1.0.1",
    "canvas": "^2.8.0",
    "color-thief-node": "^1.0.4",
    "discord.js": "^13.0.0-dev.918921e8211fc16e9b12d2502f3168264246ea22",
    "dotenv": "^8.2.0",
    "enmap": "^5.8.0",
    "klaw": "^3.0.0",
    "moment": "^2.29.0",
    "moment-timezone": "^0.5.33",
    "tsconfig-paths": "^3.9.0",
    "tspath": "^1.3.7"
  },
  "devDependencies": {
    "@types/klaw": "^3.0.1",
    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.3",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.63",
    "tsc-watch": "^4.4.0",
    "typedoc": "^0.20.36",
    "typedoc-darker-theme": "^1.1.4",
    "typescript": "4.2"
  }
}
